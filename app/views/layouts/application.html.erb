<!DOCTYPE html>
<html>
<head>
  <title>GlobeApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= flash[:error] %>
  <% if current_user %>
    <%= link_to "Sign Out", signout_path %>
  <% end %>

<%= yield %>

</body>
</html>
